{
    "parser": "babel-eslint",
    "env": {
        "browser": true,
        "node": true,
        "jest": true
    },
    "extends": "airbnb",
    "rules": {
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
        "react/forbid-prop-types": 0,
        "import/no-extraneous-dependencies": 0,
        "react/jsx-one-expression-per-line": 0,
        "no-plusplus": 0
    }
}